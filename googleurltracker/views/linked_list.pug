extends layout

block content
  - var nextPage = pageNum + 1
  - var prevPage = pageNum - 1
  h1= title
    br
    h2  Links #{pageNum*10} - #{(pageNum*10)+10} 
  br
  ul
    each url in url_list
      p 
        p Short Link:  
          a(href=url.reqUrl) #{url.reqUrl}
      | 
      p 
        p Long Link: 
          a(href=url.resUrl) #{url.resUrl}
      br
      |
    else
      li There are no books.
  p
    if (prevPage != 0)
      a(href=prevPage) Previous
    |   
    a(href=nextPage)   Next
